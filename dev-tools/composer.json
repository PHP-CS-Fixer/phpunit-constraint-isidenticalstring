{
    "require": {
        "php": "^8.5"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.50",
        "maglnet/composer-require-checker": "^4.21",
        "mi-schi/phpmd-extension": "^4.3",
        "php-cs-fixer/shim": "^3.94.2",
        "phpmd/phpmd": "^2.15"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
